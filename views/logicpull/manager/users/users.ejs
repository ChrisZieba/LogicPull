<div class="row">
	<div class="col-lg-12">

		<h1>Users</h1>

		<div class="btn-group margin-20-bottom" role="group">
			<a class="btn btn-default" href="/manager/users/add" title="Add user"><i class="fa fa-plus-square"></i> Add User</a>
		</div>

		<% for (var j = 0; j < users.length; j++) { %>
			<div class="panel panel-default">
				<div class="panel-heading">
					<div class="btn-group pull-right">
						<a class="btn btn-xs btn-default" href=""><i class="fa fa-gear"></i></a>
						<button type="button" class="btn btn-xs btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
							<span class="caret"></span>
							<span class="sr-only">Toggle Dropdown</span>
						</button>
						<ul class="dropdown-menu" role="menu">
							<li title="Remove this user"><a href="">Remove</a></li>
						</ul>
					</div>
					<i class="fa fa-user"></i> <strong><%= users[j].name %></strong>
				</div>
				<div class="panel-body">
					<i class="fa fa-envelope"></i> <%= users[j].email %><br/>
					<i class="fa fa-clock-o"></i> <em><%= users[j].created.toLocaleDateString() %> <%= users[j].created.toLocaleTimeString() %></em>
				</div>
			</div>
		<% } %>

	</div>
</div>