<div id="admin">

	<% include ../header %>

	<div class="contents-container">
		<div class="contents">
			<div class="title"><h1>Export Interview Data</h1></div>
			<div class="clear"></div>


			<div class="wide">
				<form method="post" action="">
	        		<input type="hidden" name="_csrf" value="<%= token %>"/>
	        		<input type="hidden" name="interview" value="<%= interview.id %>"/>
					<table class="add-table table">
						<tbody class="b">
				            <tr>
				            	<td>
									<p>This action will output the interview data which can be imported into another interview.</p>
									<br>
				            	</td>
				            </tr>
						</tbody>
					</table>
					<div class="clear"></div>
	        		<input type="submit" name="confirm" value="Export" class="right"/>
	        		<a class="confirm right" href="/manager"><span>Cancel, and go Back</span></a>
				   	<div class="clear"></div>
				</form>
			</div>
			<div class="clear"></div>
		</div>
	</div>
	<div class="clear"></div>
</div>